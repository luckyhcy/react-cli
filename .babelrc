{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            ">1%",
            "last 3 versions"
          ]
        }
      }
    ],
    "es2015",
    "stage-2",
    "latest",
    "react"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "syntax-dynamic-import",
    "transform-class-properties", ["import", {
      "libraryName": "antd",
      "libraryDirectory": "es",
      "style": "css"
    }]
  ]
}